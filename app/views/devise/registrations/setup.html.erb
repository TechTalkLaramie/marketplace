<section class = "banner" id = "banner-accountSetup">
	<div class = "container" id = "container-accountSetup">
		<div class = "module per100">
			<h2>Edit <%= resource_name.to_s.humanize %> Information</h2>

			<%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { method: :put }) do |f| %>
			  <div class="field margin-top">
			    <%= f.label :lat, "Latitude" %><br/>
			    <%= f.text_field :lat %>
			  </div>

			  <div class="field margin-top">
			    <%= f.label :lng, "Longitude" %><br/>
			    <%= f.text_field :lng %>
			  </div>
			 

			  <div class="actions text-right margin-top">
			    <%= f.submit "Update" , :class => "button medium"%>
			  </div>
			<% end %>
		</div>
		
	</div>
</section>
